{
  "lua": {
      "prefix": "lua",
      "body": ["lua << EOF", "$0", "EOF"]
  },
  "template plugin" : {
      "prefix": "template plugin",
      "body": [
        "\"---------------------------------------------------------------------",
        "\"   PluginName: $1",
        "\"   Github:     github.com/$2/$1",
        "",
        "\"---------------------------------------------------------------------",
        "\n\n",
        "\"---------------------------------------------------------------------",
        "\"       CONFIGS",
        "\"---------------------------------------------------------------------",
        "$0",
        "\"---------------------------------------------------------------------",
        "\"       end of CONFIGS",
        "\"---------------------------------------------------------------------",
        "\n\n\n\n\n",
        "\"---------------------------------------------------------------------",
        "\"       MAPPINGS",
        "\"---------------------------------------------------------------------",
        "",
        "\"---------------------------------------------------------------------",
        "\"       enf of MAPPINGS",
        "\"---------------------------------------------------------------------",
        ""
      ]
  },

  "Plug": {
      "prefix": "Plug",
      "body": ["Plug '$1'   \" $0"]
  }

}


